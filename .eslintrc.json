{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "indent": ["error", 2],
    "array-element-newline": ["error", { "minItems": 3 }],
    "function-paren-newline": ["error", { "minItems": 3 }]
  }
}